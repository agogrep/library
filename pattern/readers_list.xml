<?xml version="1.0"?>
<script>
    <part>
      <section>
        <parentselector>.//*[@id="readers"]</parentselector>
        <selector>.//*[@name="row"]</selector>
        <repeat>0</repeat>
      </section>
      <sql>
        <readquery></readquery>
        <writequery></writequery>
        <readgenerator>
          <scriptname>journal</scriptname>
          <param>
            <table>readers</table>
            <id_name>rid</id_name>
            <countrows>10</countrows>
          </param>
        </readgenerator>
        <argument>
          <rid><type>int</type><long></long><default></default></rid>
          <rname><type>str</type><long></long><default></default></rname>
          <descr><type>str</type><long></long><default></default></descr>
        </argument>
      </sql>
      <layout>
          <element><selector>.//*[@name="rid"]</selector><attribute></attribute><fild>rid</fild></element>
          <element><selector>.//*[@name="rid"]</selector><attribute>data-href</attribute><fild>rid</fild></element>
          <element><selector>.//*[@name="rname"]</selector><attribute></attribute><fild>rname</fild></element>
          <element><selector>.//*[@name="descr"]</selector><attribute></attribute><fild>descr</fild></element>
      </layout>
    </part>
    <part></part>
  </script>
