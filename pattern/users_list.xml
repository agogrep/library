<?xml version="1.0"?>
<script>
    <part>
      <name>journal</name>
      <section>
        <parentselector>.//*[@id="users"]</parentselector>
        <selector>.//*[@name="row"]</selector>
        <repeat>0</repeat>
      </section>
      <sql>
        <readquery></readquery>
        <writequery></writequery>
        <readgenerator>
          <scriptname>journal</scriptname>
          <param>
            <table>users</table>
            <id_name>uid</id_name>
            <countrows>10</countrows>
          </param>
        </readgenerator>
        <argument>
          <uid>
            <type>int</type>
            <long></long>
            <default></default>
          </uid>
          <login>
            <type>str</type>
            <long></long>
            <default></default>
          </login>
          <password>
            <type>str</type>
            <long></long>
            <default></default>
          </password>
          <fullname>
            <type>str</type>
            <long></long>
            <default></default>
          </fullname>
          <email>
            <type>str</type>
            <long></long>
            <default></default>
          </email>
          <descr>
            <type>str</type>
            <long></long>
            <default></default>
          </descr>
        </argument>
      </sql>

      <layout>
          <element>
            <selector>.//*[@name="uid"]</selector>
            <attribute></attribute>
            <fild>uid</fild>
          </element>
          <element>
            <selector>.//*[@name="uid"]</selector>
            <attribute>data-href</attribute>
            <fild>uid</fild>
          </element>
          <element>
            <selector>.//*[@name="login"]</selector>
            <attribute></attribute>
            <fild>login</fild>
          </element>
          <element>
            <selector>.//*[@name="fullname"]</selector>
            <attribute></attribute>
            <fild>fullname</fild>
          </element>
          <element>
            <selector>.//*[@name="email"]</selector>
            <attribute></attribute>
            <fild>email</fild>
          </element>
          <element>
            <selector>.//*[@name="descr"]</selector>
            <attribute></attribute>
            <fild></fild>
          </element>
      </layout>
    </part>
    <part></part>
  </script>
